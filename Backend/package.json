    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-fileupload": "^1.5.2",
    "file-type": "^16.5.4",
    "form-data": "^4.0.5",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
